{
  "name": "read-that-later",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build -prod --build-optimizer",
    "test": "ng test -cc -e test",
    "lint": "ng lint --fix --format codeFrame",
    "e2e": "ng e2e -prod -aot -e test",
    "e2e:watch": "watch 'ng e2e -s false -e test' src e2e --wait=1",
    "deploy": "npm run build && firebase deploy",
    "postinstall": "chmod 744 pre-commit && cp pre-commit .git/hooks/pre-commit"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.3.5",
    "@angular/compiler": "^4.3.5",
    "@angular/core": "^4.3.5",
    "@angular/forms": "^4.3.5",
    "@angular/platform-browser": "^4.3.5",
    "@angular/platform-browser-dynamic": "^4.3.5",
    "@angular/router": "^4.3.5",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/router-store": "^4.0.4",
    "@ngrx/store": "^4.0.3",
    "angularfire2": "^4.0.0-rc.2",
    "core-js": "^2.5.0",
    "firebase": "^4.3.1",
    "lodash": "^4.17.4",
    "ngrx-store-logger": "^0.2.0",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.1",
    "@angular/compiler-cli": "^4.3.5",
    "@angular/language-service": "^4.3.5",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "^2.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.24",
    "codelyzer": "~3.1.1",
    "jasmine-core": "^2.5.2",
    "jasmine-marbles": "^0.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": ">=2.1.0 <2.4.0",
    "watch": "^1.0.2"
  }
